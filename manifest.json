{
    "manifest_version": 3,
    "name": "NSFW Restriction Extension",
    "version": "1.0",
    "description": "Automatically detects and blurs NSFW images on web pages.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_title": "NSFW Restriction Extension"
    }
  }
  